{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-plusplus": 0,
      "no-useless-return": 0,
      "no-restricted-globals": 0,
      "func-names": 0,
      "no-unused-expressions": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "class-methods-use-this": 0,
      "no-useless-concat": 0
    }
  }